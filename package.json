{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "cz": "czg",
    "dev": "vite",
    "tailwind": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind-build.css --watch",
    "build": "run-p type-check \"build:*\"",
    "build:vite": "vite build",
    "build:rollup": "tsx rollup.mts",
    "build:primitive": "run-p type-check build-only",
    "gen": "bash ./scripts/gc.sh",
    "gen:store": "bash ./scripts/gstore.sh",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write --cache .",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "lint:commit": "commitlint --from $(git merge-base origin/dev HEAD) --to HEAD > ./commit-lint.txt",
    "stub": "pnpm run -r --parallel stub",
    "prepare": "husky install",
    "postinstall": "pnpm stub"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@element-plus/icons-vue": "^2.0.10",
    "@tinymce/tinymce-vue": "^5.0.1",
    "@unocss/reset": "^0.50.6",
    "@vue/apollo-composable": "4.0.0-beta.4",
    "@vuesax-alpha/icons-vue": "0.0.2-beta.2",
    "@vueuse/core": "^9.13.0",
    "@vueuse/shared": "^9.13.0",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.30",
    "file-saver": "^2.0.5",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "js-cookie": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.28",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuesax-alpha": "0.1.6-alpha-2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/file-saver": "^2.0.5",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.13.0",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue-macros/volar": "^0.8.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cz-git": "^1.4.1",
    "czg": "^1.4.1",
    "esbuild": "^0.17.11",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "expect-type": "^0.15.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "lodash-unified": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup": "^3.18.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.3",
    "type-fest": "^3.5.7",
    "typescript": "~4.7.4",
    "unocss": "^0.50.6",
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-macros": "^1.7.3",
    "vite": "^4.1.1",
    "vite-plugin-pwa": "^0.14.4",
    "vue-tsc": "^1.0.12"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}
